{% extends 'base.html.twig' %}

{% block body %}

    <h1>Réunion</h1>

{{ form_start(form) }}
    <div class="form-group">
        <input placeholder="Titre de la réunion" type="text" class="form-control">
    </div>
    <div class="form-group">
        <input placeholder="Description de la réunion" type="text" class="form-control">
    </div>
    <div class="form-group">
        <input placeholder="À faire pour la prochaine réunion" type="text" class="form-control">
    </div>
    <div class="form-group">
        <button type="submit" class="btn btn-success">Ajouter</button>
    </div>
 {{ form_end(form) }}


{% endblock %}